{
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": "./",
    "target": "ES5",
    "strict": true,
    "module": "commonJs",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "lib": ["ES5", "ES6", "ES7", "ES2017", "ES2018", "ES2019", "DOM"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
